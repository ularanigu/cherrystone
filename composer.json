{
    "name": "ularanigu/cherrystone",
    "description": "Designed to ensure security on web authentication systems.",
    "keywords": [
        "authentication-middleware",
        "authentication-system",
        "web-security",
        "php-security",
        "php72"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "doctrine/orm": "^2",
        "swiftmailer/swiftmailer": "^6",
        "symfony/yaml": "^4",
        "ramsey/uuid": "^3",
        "psr/container": "^1",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Ularanigu\\Cherrystone\\": "./src",
            "Ularanigu\\Firestorm\\": "./fs"
        },
        "files": [
            "./lib/runtime.php",
            "./lib/functions.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "vimeo/psalm": "^1|^2"
    },
    "prefer-stable": true
}
